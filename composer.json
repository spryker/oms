{
  "name": "spryker/oms",
  "autoload": {
    "psr-0": {
      "Spryker": "src/"
    },
    "psr-4": {
      "Oms\\Module\\": "tests/_support/Module/"
    }
  },
  "require": {
    "spryker/graph": "^3.0.0",
    "spryker/gui": "^3.0.0",
    "spryker/kernel": "^3.0.0",
    "spryker/mail": "^4.0.0",
    "spryker/propel-orm": "^1.0.0",
    "spryker/sales": "^5.0.0",
    "spryker/symfony": "^3.0.0",
    "spryker/util-network": "^1.1.0",
    "spryker/util-sanitize": "^2.0.0",
    "spryker/util-text": "^1.1.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/config": "*",
    "spryker/propel": "*",
    "spryker/silex": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/checkout": "If you want to use Checkout plugins you need to install spryker/checkout."
  },
  "description": "Oms bundle",
  "license": "proprietary",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "6.0.x-dev"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://code.spryker.com/repo/private"
    }
  ],
  "config": {
    "sort-packages": true
  },
  "autoload-dev": {
    "psr-0": {
      "Acceptance": "tests/",
      "Functional": "tests/",
      "Unit": "tests/"
    }
  }
}
