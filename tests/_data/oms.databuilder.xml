<?xml version="1.0"?>
<transfers
        xmlns="spryker:databuilder-01"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="spryker:databuilder-01 http://static.spryker.com/databuilder-01.xsd"
>
    <transfer name="PriceProductOffer">
        <property name="fkProductOffer" dataBuilderRule="="/>
        <property name="fkPriceType"  dataBuilderRule="="/>
        <property name="fkStore" dataBuilderRule="="/>
        <property name="fkCurrency" dataBuilderRule="="/>
        <property name="netPrice" dataBuilderRule="=100"/>
        <property name="grossPrice" dataBuilderRule="=100"/>
    </transfer>

    <transfer name="OmsProductReservation">
        <property name="reservationQuantity" dataBuilderRule="randomNumber(5)"/>
        <property name="sku" dataBuilderRule="="/>
        <property name="fkStore" dataBuilderRule="="/>
    </transfer>

</transfers>

